;\n    setOptions(data.options);\n    \n}, [])\n\n\n\nasync function deletePoll () {\n    let url = window.location.pathname.split('/')[3];\n    \n    let URL = 'https://voting-appps.herokuapp.com/api/polls/' + url;\n\n    let token = localStorage.getItem(\"jwtToken\");\n   \n    let config = {\n      headers: {\n        'Authorization': 'Bearer ' + token\n      }\n    }\n    let data = {\n        \"question\": question\n    }\n    try {\n      const res = await axios.put(URL, data, config);\n      console.log(res);\n      \n      window.location.href = \"https://voting-appps.herokuapp.com/\";\n    }\n    catch(err) {\n      console.log(err);\n    }\n  }\n  function handleChange(event) {\n      setQuestion(event.target.value);\n  }\n\n  return (\n    <div\n        style={{\n            textAlign: \"center\"\n        }}\n    >\n      <input className=\"poll-title\" style={{\n            textAlign: \"center\" \n        }}\n        onChange={handleChange}\n      value={question}></input>\n      <div className=\"buttons_center\"\n        style={{\n            textAlign: \"center\"\n        }}\n      >{answers}</div>\n      \n      <div\n        style={{\n          textAlign : \"center\",\n          padding : \"10px\",\n          marginTop: \"40px\"\n        }}\n      >\n        <button\n          style={{\n            padding: \"10px 20px\" ,\n            color: \"red\",\n            borderRadius: \"10px\",\n            boxShadow: \"10px 4px 9px rgba(0, 0, 0, 0.3)\",\n            border: \"none\"\n          }}\n          onClick={() => deletePoll()}\n        >\n          Update Polls\n        </button>   \n      </div> \n    </div>\n  );\n};\n\nexport default connect(\n  store => ({\n    poll: store.currentPoll,\n  }),\n  { vote },\n)(Poll);",{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":38,"column":15,"nodeType":"136","endLine":38,"endColumn":59},{"ruleId":"137","severity":1,"message":"138","